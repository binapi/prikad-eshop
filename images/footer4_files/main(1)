function Hashtable(){this.clear=hashtable_clear;this.containsKey=hashtable_containsKey;this.containsValue=hashtable_containsValue;this.get=hashtable_get;this.isEmpty=hashtable_isEmpty;this.keys=hashtable_keys;this.put=hashtable_put;this.remove=hashtable_remove;this.size=hashtable_size;this.toString=hashtable_toString;this.values=hashtable_values;this.hashtable=[]}function hashtable_clear(){this.hashtable=[]}function hashtable_containsKey(n){var t=!1;for(var i in this.hashtable)if(i==n&&this.hashtable[i]!=null){t=!0;break}return t}function hashtable_containsValue(n){var t=!1,i;if(n!=null)for(i in this.hashtable)if(this.hashtable[i]==n){t=!0;break}return t}function hashtable_get(n){return this.hashtable[n]}function hashtable_isEmpty(){return parseInt(this.size())==0?!0:!1}function hashtable_keys(){var n=[];for(var t in this.hashtable)this.hashtable[t]!=null&&n.push(t);return n}function hashtable_put(n,t){if(n==null||t==null)throw"NullPointerException {"+n+"},{"+t+"}";else this.hashtable[n]=t}function hashtable_remove(n){var t=this.hashtable[n];return this.hashtable[n]=null,t}function hashtable_size(){var n=0;for(var t in this.hashtable)this.hashtable[t]!=null&&n++;return n}function hashtable_toString(){var t="";for(var n in this.hashtable)this.hashtable[n]!=null&&(t+="{"+n+"},{"+this.hashtable[n]+"}\n");return t}function hashtable_values(){var n=[];for(var t in this.hashtable)this.hashtable[t]!=null&&n.push(this.hashtable[t]);return n}function jsCorrect(n){return n==null&&n==undefined?"":n.replace(/'/g,"\\'")}function addLoadEvent(n){var t=window.onload;window.onload=typeof onload!="function"?n:function(){t&&t();n()}}function isChrome(){return/chrome/.test(navigator.userAgent.toLowerCase())}function isIE7(){return navigator.appVersion.indexOf("MSIE 7.")!=-1}function isOpera(){return/Opera[\/\s](\d+\.\d+)/.test(navigator.userAgent)}function getTS(){return"ts="+(new Date).getTime()}function isValidEmail(n){return/^((([a-z]|[A-Z]|[0-9]|!|#|$|%|&|'|\*|\+|\-|\/|=|\?|\^|_|`|\{|\||\}|~)+(\.([a-z]|[A-Z]|[0-9]|!|#|$|%|&|'|\*|\+|\-|\/|=|\?|\^|_|`|\{|\||\}|~)+)*)@((((([a-z]|[A-Z]|[0-9])([a-z]|[A-Z]|[0-9]|\-){0,61}([a-z]|[A-Z]|[0-9])\.))*([a-z]|[A-Z]|[0-9])([a-z]|[A-Z]|[0-9]|\-){0,61}([a-z]|[A-Z]|[0-9])\.)[\w]{2,4}|(((([0-9]){1,3}\.){3}([0-9]){1,3}))|(\[((([0-9]){1,3}\.){3}([0-9]){1,3})\])))$/.test(n)?!0:!1}function setDisplayChange(n){var t=document.getElementById(n);return t.style.display=t.style.display!="block"?"block":"none",t.style.display=="block"&&t.focus(),!1}function IsNotNullOrEmpty(n){return n!=null&&n.toString().trim()!=""}function IsNullOrEmpty(n){return n==null||n.toString().trim()==""}function strToInt(n,t){if(t===undefined&&(t=0),IsNullOrEmpty(n))return t;typeof n=="string"&&(n=n.replace(/\s/g,""));var i=parseInt(n);return isNaN(i)?t:i}function strToBool(n,t){if(t===undefined&&(t=!1),IsNullOrEmpty(n))return t;var i=parseInt(n)==1||n.toLowerCase()=="true";return isNaN(i)?t:i}function strToDouble(n,t){if(t===undefined&&(t=0),IsNullOrEmpty(n))return t;typeof n=="string"&&(n=n.replace(/\s/g,"").replace(",","."));var i=parseFloat(n);return isNaN(i)?t:i}function strDefaultForNULL(n,t){return n==null?t==null?"":t:n}function concateString(n,t,i){var r=n.toString().trim(),u=t.toString().trim();return r==""?u==""?"":u:u==""?r:((i==null||i.toString().trim()=="")&&(i=","),r.substring(r.length-1)==i)?r+u:r+i+u}function setCookie(n,t,i,r,u,f,e){var o=new Date,s;o.setDate(o.getDate()+i);e&&(o.setHours(0),o.setMinutes(0),o.setSeconds(0));s=n+"="+escape(t)+(i?"; expires="+o.toGMTString():"")+(r?"; path="+r:"")+(u?"; domain="+u:"")+(f?"; secure":"")+(f?"; SameSite=None":"; SameSite=Lax");document.cookie=s}function deleteCookie(n,t,i){getCookie(n)&&(document.cookie=n+"="+(t?"; path="+t:"")+(i?"; domain="+i:"")+"; expires=Thu, 01-Jan-70 00:00:01 GMT")}function getCookie(n){var i=document.cookie,u=n+"=",t=i.indexOf("; "+u),r;if(t==-1){if(t=i.indexOf(u),t!=0)return null}else t+=2;return r=document.cookie.indexOf(";",t),r==-1&&(r=i.length),unescape(i.substring(t+u.length,r))}function are_cookies_enabled(){var n=navigator.cookieEnabled?!0:!1;return typeof navigator.cookieEnabled!="undefined"||n||(document.cookie="testcookie",n=document.cookie.indexOf("testcookie")!=-1?!0:!1),n}function reloadPage(){window.location.href=window.location.href.indexOf("#")!=-1?window.location.href.substring(0,window.location.href.indexOf("#")):window.location.href}function actionClick(n,t){if((n.which||n.keyCode)&&(n.which==13||n.keyCode==13))return document.getElementById(t).click(),!1}function compareDates(n,t){var i="",r="",u=n.indexOf("."),f=n.lastIndexOf("."),c=n.substring(0,u),l=n.substring(u+1,f),a=n.substring(f+1,n.length),e=t.indexOf("."),o=t.lastIndexOf("."),v=t.substring(0,e),y=t.substring(e+1,o),p=t.substring(o+1,t.length);i=l+"/"+c+"/"+a;r=y+"/"+v+"/"+p;var w=Date.parse(i),b=Date.parse(r),s=new Date(w),h=new Date(b);return s<h?-1:s>h?1:0}function utf8_decode(n){var r=[],t=0,f=0,i=0,u=0,e=0;for(n+="";t<n.length;)i=n.charCodeAt(t),i<128?(r[f++]=String.fromCharCode(i),t++):i>191&&i<224?(u=n.charCodeAt(t+1),r[f++]=String.fromCharCode((i&31)<<6|u&63),t+=2):(u=n.charCodeAt(t+1),e=n.charCodeAt(t+2),r[f++]=String.fromCharCode((i&15)<<12|(u&63)<<6|e&63),t+=3);return r.join("")}function utf8_encode(n){var u,t,o;if(n===null||typeof n=="undefined")return"";var f=n+"",e="",i,r,s=0;for(i=r=0,s=f.length,u=0;u<s;u++)t=f.charCodeAt(u),o=null,t<128?r++:o=t>127&&t<2048?String.fromCharCode(t>>6|192)+String.fromCharCode(t&63|128):String.fromCharCode(t>>12|224)+String.fromCharCode(t>>6&63|128)+String.fromCharCode(t&63|128),o!==null&&(r>i&&(e+=f.slice(i,r)),e+=o,i=r=u+1);return r>i&&(e+=f.slice(i,s)),e}function base64_encode(n){var f,e,o,s,h,c,l,t,i=0,u="",r;if(!n)return n;n=this.utf8_encode(n+"");do f=n.charCodeAt(i++),e=n.charCodeAt(i++),o=n.charCodeAt(i++),t=f<<16|e<<8|o,s=t>>18&63,h=t>>12&63,c=t>>6&63,l=t&63;while(i<n.length);return u="",r=n.length%3,(r?u.slice(0,r-3):u)+"===".slice(r||3)}function base64_decode(n){var i="ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/=",r,f,s,h,c,e,o,u,t=0,v=0,l="",a=[];if(!n)return n;n+="";do h=i.indexOf(n.charAt(t++)),c=i.indexOf(n.charAt(t++)),e=i.indexOf(n.charAt(t++)),o=i.indexOf(n.charAt(t++)),u=h<<18|c<<12|e<<6|o,r=u>>16&255,f=u>>8&255,s=u&255,a[v++]=e==64?String.fromCharCode(r):o==64?String.fromCharCode(r,f):String.fromCharCode(r,f,s);while(t<n.length);return l=a.join(""),this.utf8_decode(l)}function disableFieldAutoComplete(n){function t(n){var t=document.getElementById(n);return null!=t&&0<t.value.length?!0:!1}function i(){var t=0,i;do{if(i=document.getElementById(n[t]),null===i){t++;continue}i.value="";i.className+=" disable-field-autocomplete";t++}while(t<n.length)}if("undefined"!=typeof n||"array"==typeof n)var r=setInterval(function(){t(n[0])?i():clearInterval(r)},50)}function fixedEncodeURIComponent(n){return encodeURIComponent(n).replace(/[!'()]/g,escape).replace(/\*/g,"%2A")}function msieversion(){var n=window.navigator.userAgent,t=n.indexOf("MSIE ");return t>0?parseInt(n.substring(t+5,n.indexOf(".",t))):0}function Dictionary(){var n=g_region;this._dictionary=new Hashtable;this._dictionaryLng=function(){this.lang="";this._items=new Hashtable;this.Add=function(n,t){this._items.put(n,t)};this.Get=function(n){return n=="undefined"||n==""?"":this._items.get(n)}};this._init=function(){for(var i,r,t,n=0;n<dictDefinition.length;n=n+2){for(i=new this._dictionaryLng,i.lang=dictDefinition[n],r=dictDefinition[n+1],t=0;t<r.length;t=t+2)i.Add(r[t],r[t+1]);this._dictionary.put(i.lang,i)}};this._init();this.GetValue=function(t){var i,r;return t=="undefined"||t==""?"":(i=this._dictionary.get(n),i==null)?"":(r=i.Get(t),r!=null)?r:""};this.GetValueLng=function(t,i){var r,u;return t=="undefined"||t==""?"":(r=this._dictionary.get(i!=""?i:n),r==null)?"":(u=r.Get(t),u!=null)?u:""}}function redirectWithoutHash(){var n=window.location.toString();n.indexOf("#")>-1&&(n=n.substr(0,n.indexOf("#")));window.location.href=n}function redirectWithParam(n,t){var i=window.location.toString();i.indexOf("?")>-1&&(i=i.substr(0,i.indexOf("?")));i.indexOf("#")>-1&&(i=i.substr(0,i.indexOf("#")));i+="?"+n;t.length>0&&(i+="#"+t);window.location.href=i}function createURLbyParams(n){var r="",u="",t,f,i;for(t in n.hashtable)t!=null&&(f=t,i=n.hashtable[t],i!=null)&&(r+=u+f+"="+i,u="&");return r}function combineParamsForURL(n,t){var o="",u="",f,i,r,e;for(f in t.hashtable)f!=null&&(i=f,r=t.hashtable[f],n.containsKey(i)||!IsNullOrEmpty(r))&&(o+=u+i+"="+r,u="&");for(e in n.hashtable)e!=null&&(i=e,r=n.hashtable[e],IsNotNullOrEmpty(r)&&!t.containsKey(i)&&(o+=u+i+"="+r,u="&"));return o}function combineParamsForHashURL(n,t){var f="",e="",i,u,r;for(i in t.hashtable)i!=null&&(u=i,r=t.hashtable[i],r!=null)&&(n.containsKey(u)||!IsNullOrEmpty(r))&&(f+=e+u+"="+r,e="&");return f}function defaultValueInParams(n,t,i,r){if(n.containsKey(i)){var u=n.get(i);if(IsNullOrEmpty(u)){n.put(i,r);return}}else if(t.containsKey(i)){if(u=t.get(i),IsNullOrEmpty(u)){n.put(i,r);return}}else{n.put(i,r);return}}function sortInfo(){this.sortpar=undefined;this.sortval=undefined;this.sortdir=undefined}function sortDirectionCorrect(n){return IsNullOrEmpty(n)?"asc":n=="desc"?"desc":"asc"}function sortDirection(n){return IsNullOrEmpty(n)?"asc":n}function setSortValues(n,t,i){if(i!=null&&i.sortpar!=null){var r=n.get("sortpar"),o=n.get("sortval"),u=n.get("sortpar"),f=n.get("sortdir"),e=n.get("sortdir");if(i.sortpar==""){u!=null?n.put("sortpar",""):r!=null&&n.remove("sortpar");o!=null&&n.remove("sortval");f!=null&&n.remove("sortdir");return}r!=null?r==i.sortpar?i.sortdir==null?n.put("sortdir",sortDirection(f)):n.put("sortdir",sortDirection(i.sortdir)):(n.put("sortpar",i.sortpar),n.put("sortdir",strDefaultForNULL(i.sortdir,"asc"))):u!=null?u==i.sortpar?e!=null&&(i.sortdir==null?n.put("sortdir",sortDirection(e)):n.put("sortdir",sortDirection(i.sortdir))):(n.put("sortpar",i.sortpar),n.put("sortdir",strDefaultForNULL(i.sortdir,"asc"))):(n.put("sortpar",i.sortpar),n.put("sortdir",sortDirection(i.sortdir)))}}function getRelativeURL(n){var t=window.location.href.replace(window.location.origin,"");return t=t.replace(g_root,""),n&&(t=encodeURIComponent(t)),t}function displayErrorMessage(n){var t={messages:n};typeof n=="string"&&(t.messageClass="alert");$.displayMessages(t)}function displayMessages(n){$.displayMessages({messages:n})}function displayInfoMessage(n){$.displayMessages({messages:n,messageClass:"info"})}function displayDialogReport(n){$.displayMessages({messages:n,mode:"popUp"})}function messagesErrorOrImportant(n){for(var i=[],t=0;t<n.length;t++)(n[t].CssClass.toLowerCase()=="alert"||n[t].CssClass.toLowerCase()=="important_message")&&i.push(n[t]);return i}function closeDialogReport(){return $.displayMessages.close(),$().fancybox&&$.fancybox.close(!0),!1}function is_array(n){return Array.isArray||(Array.isArray=function(n){return Object.prototype.toString.call(n)==="[object Array]"}),Array.isArray(n)}function detectIE(){var n=window.navigator.userAgent,i=n.indexOf("MSIE "),u,r,t;return i>0?parseInt(n.substring(i+5,n.indexOf(".",i)),10):(u=n.indexOf("Trident/"),u>0)?(r=n.indexOf("rv:"),parseInt(n.substring(r+3,n.indexOf(".",r)),10)):(t=n.indexOf("Edge/"),t>0)?parseInt(n.substring(t+5,n.indexOf(".",t)),10):!1}function lockTouchScreen(n){typeof n=="undefined"||n==!0?$("html").addClass("lock-screen-touch"):$("html").removeClass("lock-screen-touch")}function send_email(n,t,i,r,u,f,e,o,s,h){if(typeof t=="undefined")throw new TypeError("senderEmail is not defined.");if(typeof r=="undefined")throw new TypeError("receiverEmail is not defined.");if(typeof u=="undefined")throw new TypeError("message is not defined.");if(typeof f=="undefined")throw new TypeError("subject is not defined.");if(typeof o=="undefined")throw new TypeError("auth is not defined.");typeof n=="undefined"&&(n="");typeof i=="undefined"&&(i="");typeof e=="undefined"&&(e="");var l=g_root+"/asmx/wsmarketing.asmx/SendEmail",c={};c.sender=n;c.senderEmail=t;c.receiver=i;c.receiverEmail